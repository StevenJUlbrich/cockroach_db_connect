# CockroachDB Connection Configuration
# Copy this file to .env and update with your actual values

# JDBC Driver Path
PG_JAR=/path/to/postgresql-42.3.3.jar

# Kerberos Configuration
KRB5_CONF=/path/to/krb5.conf
JAAS_CONF=/path/to/jaas.conf
KRB5_TICKET_CACHE=/path/to/krb5cc_USERNAME

# Database Connection
DB_HOST=your-cockroachdb-host.domain.com
DB_PORT=26300
DB_NAME=defaultdb
DB_SPN=cockroachdb

# Application Settings
APP_NAME=Python-JDBC-Kerberos
JAAS_APPLICATION_NAME=pgjdbc